{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "linebreak-style": 0,
        "max-len": 0,
        "no-console": 0,
        "no-use-before-define": 0,
        "no-plusplus": 0
    }
}
